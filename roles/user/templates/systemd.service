[Unit]
Description = User Service
[Service]
User=roboshop
Environment=REDIS_HOST=redis-{{ ENV }}.int.chaitu.net
Environment=MONGO_URL="mongodb:/mongodb-{{ ENV }}.int.chaitu.net/:27017/users"
ExecStart=/bin/node /home/roboshop/user/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target